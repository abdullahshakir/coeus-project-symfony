{% extends 'layouts/seller.base.html.twig' %}

{% block title %}{{ 'seller.dashboard.title'|trans }}{% endblock %}

{% block page_header %}
<div class="page-header">
    <div class="page-block">
        <div class="row align-items-center">
            <div class="col-md-8">
                <div class="page-header-title">
                    <h5 class="m-b-10"> {{ 'seller.dashboard.heading'|trans }}</h5>
                    <p class="m-b-0">{{ 'seller.dashboard.subheading'|trans }}</p>
                </div>
            </div>
            <div class="col-md-4">
                <ul class="breadcrumb-title">
                    <li class="breadcrumb-item">
                        <a href="#"> <i class="fa fa-home"></i> </a>
                    </li>
                    <li class="breadcrumb-item"><a href="#!">{{ 'seller.dashboard.heading'|trans }}</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block main %}
<div class="pcoded-inner-content">
    <div class="main-body">
        <div class="page-wrapper">
        </div>
        <div id="styleSelector"> </div>
    </div>
</div>
{% endblock %}