{% extends 'layouts/admin.base.html.twig' %}

{% block title %}Category{% endblock %}

{% block main %}
<div class="col-sm-12">
    <!-- Basic Form Inputs card start -->
    <div class="card">
        <div class="card-header">
            <h5>Category</h5>
        </div>
        <div class="card-block">

            <table class="table">
                <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ category.id }}</td>
                    </tr>
                    <tr>
                        <th>Name</th>
                        <td>{{ category.name }}</td>
                    </tr>
                    <tr>
                        <th>Status</th>
                        <td>{{ category.status }}</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
{% endblock %}